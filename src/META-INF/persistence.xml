<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="BibliotecaSantiPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>entitys.ControlPrecioMulta</class>
    <class>recursos1.entitys.Revista</class>
    <class>recursos1.entitys.Periodico</class>
    <class>recursos1.entitys.Libro</class>
    <class>recursos1.entitys.MateriaPorEnciclopedia</class>
    <class>recursos1.entitys.MateriaPorLibro</class>
    <class>recursos1.entitys.MateriaPorRevista</class>
    <class>recursos1.entitys.Mapa</class>
    <class>recursos1.entitys.Diccionario</class>
    <class>recursos1.entitys.Materia</class>
    <class>recursos1.entitys.MateriaPorDiccionario</class>
    <class>recursos1.entitys.Enciclopedia</class>
    <class>recursos1.entitys.AutorPorLibro</class>
    <class>recursos1.entitys.AutorPorEnciclopedia</class>
    <class>recursos1.entitys.AutorEnciclopedia</class>
    <class>recursos1.entitys.CategoriaColeccion</class>
    <class>recursos1.entitys.AutorDiccionario</class>
    <class>recursos1.entitys.AutorPorDiccionario</class>
    <class>recursos1.entitys.AutorLibro</class>
    <class>usuario.entitys.Bibliotecario</class>
    <class>usuario.entitys.Profesor</class>
    <class>usuario.entitys.Estudiante</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/PruebaBiblioteca"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="1234"/>
    </properties>
  </persistence-unit>
</persistence>
